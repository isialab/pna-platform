}

Table istituzione [headercolor: #175e7a] {
	id_istituzione int [ pk, increment, unique ]
	id_tipologia_istituzione int [ not null ]
	nome varchar(255) [ not null ]
	citta varchar(100)
	sede varchar(100) [ note: 'Principale/Decentrata' ]
	status varchar(100) [ note: 'Pubblico/Statale, Privato...' ]
}

Table immagine [headercolor: #175e7a] {
	id_immagine int [ pk, increment, unique ]

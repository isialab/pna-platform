Table progetto [headercolor: #175e7a] {
	id_progetto int [ pk, increment, unique ]
	id_edizione int [ not null ]
	id_istituzione int [ not null ]
	titolo varchar(255) [ not null, note: 'Nome progetto' ]
	sottotitolo varchar(255) [ note: 'Sottotitolo o specifica' ]
	descrizione varchar(500) [ not null, note: 'Sinossi/Concept/Statement max 500 caratteri spazi inclusi' ]
	tipo_progetto int [ not null, note: 'Progetto esame, Tesi, Ricerca' ]
	anno int [ not null ]
	immagini_01_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	immagini_02_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	immagini_03_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	immagini_04_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	selezionato boolean [ note: 'Ha passato la selezione' ]
	vincitore boolean [ note: 'Vincitore finale' ]
	data_invio datetime [ default: `now()` ]
	stato_candidatura varchar(50) [ default: 'Bozza', note: 'Inviata, Accettata, Incompleta' ]
}

Table candidature [headercolor: #175e7a] {
	id_candidatura int [ pk, increment, unique ]
	id_studente int [ not null, note: 'Studente Capofila / Lead Student' ]
	id_istituzione int [ not null ]
	id_edizione int [ not null ]
	id_premio int [ not null ]
	id_sezione int [ not null ]
	id_sottosezione int [ note: 'Eventuale sottosottosezione' ]
	data_invio datetime [ default: `now()` ]
	stato_candidatura varchar(50) [ default: 'Bozza', note: 'Inviata, Accettata, Incompleta' ]
}

Table candidatura_studente [headercolor: #175e7a] {
	id_candidatura int [ pk ]
	id_studente int

	indexes {
		(id_candidatura, id_studente) [ name: 'candidatura_studente_index_0' ]
	}
}

Table studente [headercolor: #175e7a] {
	id_studente int [ pk, increment, unique ]
	nome varchar(100) [ not null ]
	cognome varchar(100) [ not null ]
	email varchar(100) [ not null ]
	password varchar(255) [ not null ]
	//Dati accademici
	codice_fiscale varchar(255) [ not null ]
	data_nascita date [ not null, note: 'Es. 01/12/2000' ]
	luogo_nascita varchar(255) [ not null, note: 'Es. Roma' ]
	nazionalita varchar(100) [ not null, note: 'Es. Italiana' ]
	residenza varchar(255) [ not null ]
	indirizzo varchar(255) [ not null ]
	telefono varchar(50) [ not null, note: 'Es. +39 000000000' ]
}

Table docente [headercolor: #175e7a] {
	id_docente int [ pk, increment, unique ]
	nome varchar(100) [ not null ]
	cognome varchar(100) [ not null ]
	email varchar(100) [ not null ]
	password varchar(255) [ not null ]
}

Table tipologia [headercolor: #175e7a] {
	id_tipologia int [ pk, increment, unique ]
	nome varchar(100) [ not null, note: 'Es. Tesi, Ricerca' ]
}

Table tag [headercolor: #175e7a] {
	id_tag int [ pk, increment, unique ]
	nome varchar(50) [ not null ]
}

Table performer [headercolor: #175e7a] {

	}
}

Table progetto_docente [headercolor: #175e7a] {
	id_progetto int [ pk ]
	id_docente int [ note: 'Mappa il campo Teacher(s)/Docente/i' ]
	corso_disciplina varchar(50) [ not null, note: 'Nome della disciplina dell\'insegnamento' ]
	corso_livello varchar(50) [ not null, note: 'I livello/II livello' ]
	corso_denominazione varchar(50) [ not null, note: 'Nome del corso accademico' ]

	indexes {
		(id_progetto, id_docente) [ name: 'progetto_docente_index_2' ]
	}
}

Table progetto_tag [headercolor: #175e7a] {
	id_progetto int [ pk ]
	id_tag int

	indexes {
		(id_progetto, id_tag) [ name: 'progetto_tag_index_3' ]
	}
}

Table progetto_performer [headercolor: #175e7a] {
	id_progetto int [ pk ]
	id_performer int

	indexes {
		(id_progetto, id_performer) [ name: 'progetto_performer_index_4' ]
	}
}

Table studente_istituzione [headercolor: #175e7a] {
	id_studente int [ pk, unique ]
	id_istituzione int
	//Dati accademici
	matricola varchar(100) [ not null ]
	corso_anno varchar(50) [ not null, note: 'I anno/II anno/III anno' ]
	corso_livello varchar(50) [ not null, note: 'I livello/II livello' ]
	corso_denominazione varchar(50) [ not null, note: 'Nome del corso accademico' ]


	design_immagini_02_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	design_immagini_03_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profilo colore RGB' ]
	design_immagini_04_path varchar(255) [ note: 'Formato jpg/png/tiﬀ, formato 16:9 o 4:3 o 1:1, lato lungo 2480 pixel, risoluzione 300 dpi con profillo colore RGB' ]
